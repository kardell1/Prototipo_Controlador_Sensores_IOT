#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/bin/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/bin/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/mqtt@5.10.1/node_modules:/home/kardell/Documents/Proyects_vsCode/Proyecto_Controlador_Sensores_Prototipo/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/bin/pub.js" "$@"
else
  exec node  "$basedir/../../build/bin/pub.js" "$@"
fi
